{
  "description": "",
  "indiv": {
    "net": {
      "class": "ResNet",
      "params": {
        "arch": "resnet50",
        "quantAct": false,
        "quantWeights": true,
        "quantSkipFirstLayer": true,
        "quantSkipLastLayer": true,
        "weightInqStrategy": "magnitude-SRQ",
        "weightInqLevels": 2,
        "weightInqQuantInit": "uniform-perCh-l2opt",
        "pretrained": true,
        "weightInqSchedule": {
           "0": 0.9,
           "1": 0.9
        }
      },
      "pretrained": null
    },
    "loss_function": {
      "class": "CrossEntropyLoss",
      "params": {}
    }
  },
  "treat": {
    "thermostat": {
      "params": {
        "noise_schemes": {},
        "bindings": []
      }
    },
    "optimizer": {
      "class": "Adam",
      "params": {"lr": 1e-3}
    },
    "lr_scheduler": {
      "class": "ReduceLROnPlateau",
      "step_metric": "valid_metric",  
      "params": {}
    },
    "data": {
      "augment": "torchvision2",
      "bs_train": 256,
      "bs_valid": 64
    },
    "max_epoch": 155
  },
  "protocol": {
    "update_metric": "valid_metric",
    "metric_period": 1
  }
}
