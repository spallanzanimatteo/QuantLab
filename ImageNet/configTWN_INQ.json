{
  "description": "",
  "indiv": {
    "net": {
      "class": "AlexNet",
      "params": {
        "capacity": 1,
        "quantAct": false,
        "quantWeights": false,
        "quantSkipFirstLayer": true
      },
      "pretrained": null
    },
    "loss_function": {
      "class": "CrossEntropyLoss",
      "params": {}
    }
  },
  "treat": {
    "thermostat": {
      "params": {
        "noise_schemes": {},
        "bindings": []
      }
    },
    "optimizer": {
      "class": "Adam",
      "params": {"lr": 1e-3}
    },
    "lr_scheduler": {
      "class": "HandScheduler",
      "params": {"schedule": {"700": 1e-1}}
    },
    "data": {
      "augment": true,
      "bs_train": 256,
      "bs_valid": 64
    },
    "max_epoch": 1000
  },
  "protocol": {
    "update_metric": "valid_metric",
    "metric_period": 1
  }
}
