{
  "description": "randomDescription",
  "indiv": {
    "net": {
        "class": "VGG",
        "params": {
        "quantAct": false, 
        "quantWeights": true,
        "quantSkipFirstLayer": true,
        "weightInqBits": 2, 
        "weightInqStrategy": "magnitude-SRQ", 
        "stepEveryEpoch": true,
        "weightInqSchedule": {
          "100": 0.9,
          "150": 0.95,
          "200": 0.975,
          "250": 0.9875,
          "320": 0.99375,
          "400": 1.0
        }
      },
      "pretrained": null
    },
    "loss_function": {
      "class": "CrossEntropyLoss",
      "params": {}
    }
  },
  "treat": {
    "thermostat": {
      "params": {
        "noise_schemes": {},
        "bindings": []
      }
    },
    "optimizer": {
      "class": "Adam",
      "params": {"lr": 1e-3}
    },
    "lr_scheduler": {
        "class": "HandScheduler",
        "params": {
          "schedule": {
              "75": 0.1,
              "101": 1.0,
              "370": 0.1,
              "420": 0.01
          }
        }
    },
    "data": {
      "augment": true,
      "valid_fraction": 0.1,
      "bs_train": 256,
      "bs_valid": 64
    },
    "max_epoch": 450
  },
  "protocol": {
    "update_metric": "valid_metric",
    "metric_period": 1
  }
}
